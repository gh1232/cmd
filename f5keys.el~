(global-set-key (kbd "<f5>") 'nil)
(global-set-key (kbd "<f5>q") 'save-buffers-kill-emacs)
(global-set-key (kbd "<f5>s") 'save-buffer)
(global-set-key (kbd "<f5>c") 'delete-char)
(global-set-key (kbd "<f5>w") 'kill-word)
(global-set-key (kbd "<f5>l") 'kill-line)
(global-set-key (kbd "<f5>u") 'undo)
(global-set-key (kbd "<f5>n") 'next-line)
(global-set-key (kbd "<f5>p") 'previous-line)
(global-set-key (kbd "<f5>[") 'scroll-up)
(global-set-key (kbd "<f5>]") 'scroll-down)
(global-set-key (kbd "<f5>j") 'beginning-of-line)
(global-set-key (kbd "<f5>k") 'end-of-line)
(global-set-key (kbd "<f5>.") 'forward-char)
(global-set-key (kbd "<f5>,") 'backward-char)
(global-set-key (kbd "<f5>'") 'forward-word)
(global-set-key (kbd "<f5>;") 'backward-word)
(global-set-key (kbd "<f5>-") 'beginning-of-buffer)
(global-set-key (kbd "<f5>=") 'end-of-buffer)
(global-set-key (kbd "<f5>g") 'goto-line)
(global-set-key (kbd "<f5>/") 'isearch-foward)
(global-set-key (kbd "<f5>?") 'isearch-backward)
(global-set-key (kbd "<c-f4>/") 'query-replace)
(global-set-key (kbd "<f5>m") 'set-mark-command)
(global-set-key (kbd "<f5>y") 'command-yank)
(global-set-key (kbd "<f5>r") 'kill-region)
(global-set-key (kbd "<f5>f") 'find-file)
(global-set-key (kbd "<f5>b") 'switch-to-buffer)
(global-set-key (kbd "<f5>|") 'split-window-vertically)
(global-set-key (kbd "<f5>1") 'delete-other-windows)
(global-set-key (kbd "<f5>o") 'other-window)
  (global-set-key (kbd "<m-f5>/") 'query-replace-regexp)

